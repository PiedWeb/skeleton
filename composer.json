{
    "type": "project",
    "name": "piedweb/skeleton",
    "description": "Install Pied Web CMS and default directories to build your app.",
    "homepage": "https://piedweb.com",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "symfony/flex": "^1.1",
        "symfony/skeleton": "^4.1",
        "piedweb/cms-bundle": "dev-master@dev"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.7",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "sort-packages": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
     "authors": [
        {
            "name": "Robin D.",
            "email": "contact@robin-d.fr",
            "homepage": "https://www.robin-d.fr/"
        }
    ]
}
